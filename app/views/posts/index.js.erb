$posts = '<%= j render @posts %>'
$('[data-behavior="post-insert"]').append($posts);
collectionPostEditDropdown();
$(".best_in_place").best_in_place();
<% if @posts.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @posts %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
  $('.no-more').delay(1000).fadeIn(1500);
<% end %>