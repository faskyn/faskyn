<table class="table table-bordered">
  <thead>
    <tr>
      <th class="col-md-9">Links</th>
      <th class="col-md-3">Sent at</th>
    </tr>
  </thead>
  <tbody class="linkbody">
    <%= render partial: 'common_medias/message_link_body' %>
