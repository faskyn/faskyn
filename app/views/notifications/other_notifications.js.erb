$(".other-notifications-index").append('<%= j render @other_notifications %>');
<% if @other_notifications.next_page %>
  $(".pagination").replaceWith('<%= j will_paginate @other_notifications %>');
<% else %>
  $(window).off('scroll');
  $(".pagination").remove();
  $(".no-more-notifications").delay(1000).fadeIn(1500);
<% end %>