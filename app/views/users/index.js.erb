$('[data-behavior="user-profile-index"]').append('<%= j render partial: "users/user" %>');
<% if @users.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate partial: "users/user" %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
  $('.no-more').delay(1000).fadeIn(1500);
<% end %>